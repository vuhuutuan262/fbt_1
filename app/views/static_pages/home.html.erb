<% provide :title, t(".title") %>
<div id="wrapper">
  <div id="body">
    <div id="bigPic">
      <% @places.each do |f| %>
        <%= link_to "" do %>
          <%= image_tag "#{f.url_image}",
            size: Settings.size_big_pic %>
        <% end %>
      <% end %>
    </div>
  </div>
  <div class="clearfix"></div>
  <div id="push"></div>
</div>
<div style="width:100%; height: 280px">
  <% @reviews.each do |f| %>
    <%= render "reviews/reviews", f: f %>
  <% end %>
</div>
<div id="footer"></div>
